{
  "name": "@adriangalilea/utils",
  "version": "0.0.9",
  "description": "TypeScript utilities - logger, currency, formatter, and more",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/browser.js",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "default": "./dist/browser.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "default": "./dist/runtime.js"
    },
    "./format": {
      "types": "./dist/universal/format.d.ts",
      "default": "./dist/universal/format.js"
    },
    "./currency": {
      "types": "./dist/universal/currency/index.d.ts",
      "default": "./dist/universal/currency/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "test": "tsx tests/test.ts",
    "test:runtime": "tsx tests/runtime-test.ts",
    "test:browser": "open tests/browser-test.html",
    "example": "tsx tests/example.ts",
    "update-crypto": "tsx src/universal/currency/download-crypto-list.ts"
  },
  "keywords": [
    "typescript",
    "utils",
    "log",
    "currency",
    "format"
  ],
  "author": "Adrian Galilea <adriangalilea@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "chalk": "^5.6.0"
  }
}